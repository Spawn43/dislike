{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16":   "/images/icon.ico",
      "32":   "/images/icon.ico",
      "48":   "/images/icon.ico",
      "128":  "/images/icon.ico"
    }
  },
  "icons": {
    "16":   "/images/icon.ico",
    "32":   "/images/icon.ico",
    "48":   "/images/icon.ico",
    "128":  "/images/icon.ico"
  }
}